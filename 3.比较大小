//    <--------比较输出最大数-------->
int main()
{
	int a, b, c,max;
	scanf("%d%d%d", &a, &b, &c);
	printf("%d\n",(a>b?a>c?a:b>c?b:c:b?b:c));
	return 0;
}
//    <--------比较大小输出排序-------->
int main()
{
	int a, b, c,t;
	scanf("%d%d%d", &a, &b, &c);
	if (a>b)
	{
		t = a,a = b,b = t;
	}
	if (a>c)
	{
		t = a,a = c,c = t;
	}
	if (b>c)
	{
		t = b,b = c, c = t;
	}
	printf("%d->%d->%d\n", a, b, c);
	return 0;
}
